{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "AccountRoute": {
        "ClusterId": "Account",
        "Match": {
          "Path": "api/account/{**catch-all}"
        }
      },
      "ProfileRoute": {
        "ClusterId": "Account",
        "Match": {
          "Path": "api/profile/{**catch-all}"
        }
      },
      "NotificationsRoute": {
        "ClusterId": "Notifications",
        "Match": {
          "Path": "api/notifications/{**catch-all}"
        }
      },
      "SastImgRoute": {
        "ClusterId": "Sastimg",
        "Match": {
          "Path": "api/sastimg/{**catch-all}"
        }
      },
      "SNSRoute": {
        "ClusterId": "SNS",
        "Match": {
          "Path": "api/sns/{**catch-all}"
        }
      },
      "SquareRoute": {
        "ClusterId": "Square",
        "Match": {
          "Path": "api/square/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "Account": {
        "Destinations": {
          "1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "Notifications": {
        "Destinations": {
          "1": {
            "Address": "http://localhost:5002/"
          }
        }
      },
      "Sastimg": {
        "Destinations": {
          "1": {
            "Address": "http://localhost:5003/"
          }
        }
      },
      "SNS": {
        "Destinations": {
          "1": {
            "Address": "http://localhost:5004/"
          }
        }
      },
      "Square": {
        "Destinations": {
          "1": {
            "Address": "http://localhost:5005/"
          }
        }
      }
    }
  }
}