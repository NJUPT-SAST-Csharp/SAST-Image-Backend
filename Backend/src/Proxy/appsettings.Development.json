{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "AccountRoute": {
        "ClusterId": "Account",
        "Match": {
          "Path": "api/account/{**catch-all}"
        }
      },
      "ProfileRoute": {
        "ClusterId": "Account",
        "Match": {
          "Path": "api/profile/{**catch-all}"
        }
      },
      "NotificationsRoute": {
        "ClusterId": "Notifications",
        "Match": {
          "Path": "api/notifications/{**catch-all}"
        }
      },
      "SastImgRoute": {
        "ClusterId": "SastImg",
        "Match": {
          "Path": "api/sastimg/{**catch-all}"
        }
      },
      "SNSRoute": {
        "ClusterId": "SNS",
        "Match": {
          "Path": "api/sns/{**catch-all}"
        }
      },
      "StorageRoute": {
        "ClusterId": "Storage",
        "Match": {
          "Path": "api/storage/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "Account": {
        "Destinations": {
          "1": {
            "Address": "http://Account"
          }
        }
      },
      "Notifications": {
        "Destinations": {
          "1": {
            "Address": "http://Notifications"
          }
        }
      },
      "SastImg": {
        "Destinations": {
          "1": {
            "Address": "http://SastImg"
          }
        }
      },
      "SNS": {
        "Destinations": {
          "1": {
            "Address": "http://SNS"
          }
        }
      },
      "Square": {
        "Destinations": {
          "1": {
            "Address": "http://Square"
          }
        }
      },
      "Storage": {
        "Destinations": {
          "1": {
            "Address": "http://Storage"
          }
        }
      }
    }
  }
}